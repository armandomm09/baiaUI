{
  "expo": {
    "name": "BAIA",
    "slug": "Baia",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/BaiaLogoWBG.png",
    "scheme": "BAIA",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/BaiaLogoWBG.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.baiaUI.galliard",
      "bundleIdentifier": "com.baiaUI.galliard"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/BaiaLogoWBG.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "58c3bc68-8f6e-4ae0-9036-5bfe159201c6"
      }
    }
  }
}
